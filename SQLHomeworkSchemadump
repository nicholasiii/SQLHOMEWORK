CREATE VIEW public.employee AS
 SELECT employees.employee,
    employees.first_name,
    employees.last_name,
    departments.dept_name,
    dept_emp.dept_no
   FROM ((public.employees
     JOIN public.dept_emp ON ((employees.employee = dept_emp.employee)))
     JOIN public.departments ON (((departments.dept_no)::text = (dept_emp.dept_no)::text)));


ALTER TABLE public.employee OWNER TO postgres;

--
-- TOC entry 203 (class 1259 OID 17032)
-- Name: managers; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.managers AS
 SELECT dept_manager.dept_no,
    departments.dept_name,
    dept_manager.employee,
    employees.last_name,
    employees.first_name,
    dept_manager.from_date,
    dept_manager.to_date
   FROM ((public.dept_manager
     JOIN public.employees ON ((employees.employee = dept_manager.employee)))
     JOIN public.departments ON (((departments.dept_no)::text = (dept_manager.dept_no)::text)));


ALTER TABLE public.managers OWNER TO postgres;

